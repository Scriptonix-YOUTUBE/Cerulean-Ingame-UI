
print'\nSynapse UI: Version 1.2\nloaded' 



local CoreGui = game:GetService'CoreGui' 

local RobloxLoadingGui = CoreGui:WaitForChild'RobloxLoadingGui' 

local BlackFrame = RobloxLoadingGui:WaitForChild'BlackFrame' 

local LoadingImage = BlackFrame:WaitForChild'GraphicsFrame':WaitForChild'LoadingImage' 

local InfoFrame = BlackFrame:WaitForChild'InfoFrame' 

local PlaceIcon = InfoFrame:WaitForChild'PlaceIcon'

local CreatorLabel = InfoFrame:WaitForChild'CreatorLabel'

local PlaceLabel = InfoFrame:WaitForChild'PlaceLabel'

local UiMessage = InfoFrame:WaitForChild'UiMessageFrame':WaitForChild'UiMessage'

local UIListLayout = InfoFrame:WaitForChild'UIListLayout'

local PlaceThumbnail = PlaceIcon:Clone()



local function getthumb(n)
	local yes,re = pcall(function()
		PlaceThumbnail.Image = 'https://assetgame.roblox.com/Game/Tools/ThumbnailAsset.ashx?aid='..n..'&fmt=png&wd=420&ht=420'
	end)
	if not yes then
		print(re)
	end
end

local function Transparency(o)
	BlackFrame:GetPropertyChangedSignal'BackgroundTransparency':Connect(function()
		local Transparency = BlackFrame.BackgroundTransparency
		if o:IsA('TextLabel') then
			o.TextTransparency = Transparency
		else
			o.BackgroundTransparency = Transparency
		end
	end)
end

local function createOtherFrame(m)
	local yes, re = pcall(function()
		local desc = game:GetService('MarketplaceService'):GetProductInfo(game.PlaceId).Description


		local OtherFrame = Instance.new("Frame")
		local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
		local TextLabel = Instance.new("TextLabel")
		local UiMessagePadding = Instance.new("UIPadding")


		OtherFrame.Name = "OtherFrame"
		OtherFrame.Parent = m
		OtherFrame.AnchorPoint = Vector2.new(1, 1)
		OtherFrame.BackgroundColor3 = Color3.fromRGB(43,43,43)
		OtherFrame.BorderColor3 = Color3.fromRGB(43,43,43)
		OtherFrame.BorderSizePixel = 5
		OtherFrame.Position = UDim2.new(0.892091691, 0, 0.598457098, 0)
		OtherFrame.Size = UDim2.new(0.240514442, 0, 0.512000084, 0)
		OtherFrame.ZIndex = 2

		UIAspectRatioConstraint.Parent = OtherFrame

		TextLabel.Parent = OtherFrame
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.BackgroundTransparency = 1.000
		TextLabel.Position = UDim2.new(0.0377249122, 0, 0.0214355346, 0)
		TextLabel.Size = UDim2.new(0, 291, 0, 301)
		TextLabel.ZIndex = 9999
		TextLabel.Text = desc
		TextLabel.TextColor3 = Color3.fromRGB(239, 239, 239)
		TextLabel.TextSize = 14.000
		TextLabel.TextStrokeTransparency = 0.000
		TextLabel.TextWrapped = true
		TextLabel.TextXAlignment = Enum.TextXAlignment.Left
		TextLabel.TextYAlignment = Enum.TextYAlignment.Top

		UiMessagePadding.Name = "UiMessagePadding"
		UiMessagePadding.Parent = OtherFrame
		UiMessagePadding.PaddingBottom = UDim.new(0, 25)

		Transparency(OtherFrame)
		Transparency(TextLabel)
	end)

	if not yes then
		print(re)
	end
end



createOtherFrame(BlackFrame)


PlaceThumbnail.Size = UDim2.new(.366,0,.55,0)

PlaceThumbnail.Name = 'thumbstick'

PlaceThumbnail.UISizeConstraint.MaxSize = Vector2.new(2000,500)

PlaceThumbnail.UIAspectRatioConstraint.AspectRatio = 2

PlaceThumbnail.ZIndex = 99999999

getthumb(tostring(game.PlaceId))


UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center

UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Left

UIListLayout.FillDirection = Enum.FillDirection.Vertical


InfoFrame.Position = UDim2.new(0.5,0,0.5,0)

InfoFrame.Size = UDim2.new(.817,0,.951,0)

InfoFrame.UIAspectRatioConstraint.AspectRatio = 2


UiMessage.Font = Enum.Font.Nunito

UiMessage.TextXAlignment = Enum.TextXAlignment.Left


CreatorLabel.TextXAlignment = Enum.TextXAlignment.Left

CreatorLabel.Font = Enum.Font.Code

CreatorLabel.TextColor3 = Color3.fromRGB(112, 112, 112)

CreatorLabel.TextStrokeTransparency = 1


PlaceLabel.Font = Enum.Font.SourceSansSemibold

PlaceLabel.TextColor3 = Color3.fromRGB(255, 255, 255)

PlaceLabel.TextXAlignment = Enum.TextXAlignment.Left

PlaceLabel.Size = UDim2.new(1,0,.09,80)


PlaceIcon.BorderColor3 = Color3.fromRGB(53,53,53)

PlaceIcon.BorderSizePixel = 3

PlaceIcon.BackgroundTransparency = 0

PlaceIcon.BackgroundColor3 = Color3.fromRGB(0,0,0)

PlaceIcon.UISizeConstraint.MaxSize = Vector2.new(500,500)

Transparency(PlaceIcon)


LoadingImage.BackgroundColor3 = Color3.fromRGB(200,200,200)

LoadingImage.Image = 'rbxassetid://2093843497' 

LoadingImage.ImageColor3 = Color3.fromRGB(180,180,180)


BlackFrame.BackgroundColor3 = Color3.fromRGB(39,39,39) 



local TopBar = CoreGui:WaitForChild'TopBar'

local Icon = TopBar:WaitForChild'TopBarFrame':WaitForChild'LeftFrame':WaitForChild'MenuIcon':WaitForChild'Background':WaitForChild'Icon'

local TweenService = game:GetService'TweenService'

local UIS = game:GetService'UserInputService'

local i = 1

local t = true


print'\nSynapse x loading screen done'


Icon.Image = 'rbxassetid://2530517466'


local function createGUI()

	local ScreenGui = Instance.new("ScreenGui")
	local b = Instance.new("Frame")
	local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
	local UISizeConstraint = Instance.new("UISizeConstraint")
	local ImageButton = Instance.new("ImageButton")


	ScreenGui.Parent = game.CoreGui
	ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	b.Name = "b"
	b.Parent = ScreenGui
	b.AnchorPoint = Vector2.new(1, 1)
	b.BackgroundTransparency = 1.000
	b.BorderSizePixel = 0
	b.Position = UDim2.new(0, 0, 1, 0)
	b.Size = UDim2.new(1.49300003, 0, 1.61399996, 0)
	b.ZIndex = 2

	UIAspectRatioConstraint.Parent = b

	UISizeConstraint.Parent = b
	UISizeConstraint.MaxSize = Vector2.new(100, 100)

	ImageButton.Parent = b
	ImageButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ImageButton.BackgroundTransparency = 1.000
	ImageButton.Size = UDim2.new(1, 0, 1, 0)
	ImageButton.Image = "rbxassetid://2093843497"
	
	return b
end

local function open(g)
	if t then
		t = false
		local p = UDim2.new(.073,0,.998,0)
		local tween = TweenService:Create(g,TweenInfo.new(2),{Position = p})
		tween:Play()
		tween.Completed:Wait()
		t = true
	end
end

local function close(g)
	if t then
		t = false
		local p = UDim2.new(0,0,1,0)
		local tween = TweenService:Create(g,TweenInfo.new(2),{Position = p})
		tween:Play()
		tween.Completed:Wait()
		t = true
	end
end

local function rotate(g)
	if t then
		t = false
		local p = g.Rotation == 0 and 360 or 0
		local tween = TweenService:Create(g,TweenInfo.new(2),{Rotation = p})
		tween:Play()
		tween.Completed:Wait()
		t = true
	end
end


local gui = createGUI()

wait(3)

open(gui)

wait(3)

UIS.InputBegan:Connect(function(key)
	if (key.KeyCode == Enum.KeyCode.LeftControl and UIS:IsKeyDown(Enum.KeyCode.G)) or (key.KeyCode == Enum.KeyCode.G and UIS:IsKeyDown(Enum.KeyCode.LeftControl)) then
		i = (i+1)%2
		if i == 1 then
			open(gui)
		else 
			close(gui)
		end
	end
end)


gui.ImageButton.MouseButton1Click:Connect(function()
	if t then
		keypress(Enum.KeyCode.Insert) 
		keyrelease(Enum.KeyCode.Insert) 
		rotate(gui)
	end
end)
